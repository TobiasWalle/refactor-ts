{
  "name": "refactor-ts",
  "version": "0.0.0",
  "description": "Refactoring utitity for typescript projects.",
  "main": "dist/index.js",
  "bin": "dist/bin/refactor-ts.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=12"
  },
  "repository": "https://github.com/TobiasWalle/refactor-ts",
  "author": "Tobias Walle",
  "license": "MIT",
  "scripts": {
    "refactor-ts": "ts-node -P tsconfig.json ./src/bin/refactor-ts.ts",
    "test": "jest",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "build": "tsc -p tsconfig.json",
    "install:locally": "yarn prepublish && yarn global add file:$PWD",
    "prepublish": "yarn build"
  },
  "dependencies": {
    "case": "^1.6.3",
    "inquirer": "^7.3.2",
    "inquirer-fuzzy-path": "^2.3.0",
    "micromatch": "^4.0.2",
    "to-absolute-glob": "^2.0.2",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@types/inquirer": "^7.3.0",
    "@types/jest": "^26.0.5",
    "@types/micromatch": "^4.0.1",
    "@types/node": "^14.0.24",
    "@types/react": "^16.9.43",
    "@types/to-absolute-glob": "^2.0.0",
    "jest": "^26.1.0",
    "react": "^16.13.1",
    "ts-jest": "^26.1.3",
    "ts-morph": "^7.1.3",
    "ts-node": "^8.10.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.7"
  }
}
